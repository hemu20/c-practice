#include<stdio.h>
#include<conio.h>
main()
{
int a[10],i,n;
clrscr();
printf("enter the number of variables to be used\n");
scanf("%d",&n);
for(i=0;i<n;i++)
{
printf("enter the value of a[%d]\n",i);
scanf("%d",&a[i]);
}
printf("given array is\n");
for(i=0;i<n;i++)
{
printf("a[%d]=%d\n",i,a[i]);
}
}
